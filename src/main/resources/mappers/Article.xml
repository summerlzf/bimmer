<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- "namespace" must be the path of interface, or show error “is not known to the MapperRegistry” when launch -->
<mapper namespace="com.kedu.bimmer.dao.ArticleDAO">

	<!-- "id" must be as same as method name of the interface -->
	<select id="query" resultType="com.kedu.bimmer.model.Article" parameterType="map">
		<![CDATA[ select * from Article order by createTime desc limit 10 ]]>
	</select>

</mapper>